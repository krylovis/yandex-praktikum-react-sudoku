{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "dev:ssr": "tsc --project tsconfig.server.json && cross-env NODE_ENV=development node server/index.js",
    "build:vite": "tsc && vite build",
    "build:ssr": "tsc --project tsconfig.server.json && vite build && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "node server/index.js",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest ./",
    "prepare": "husky",
    "stylelint": "npx stylelint ./**/*.scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all lint test"
    }
  },
  "dependencies": {
    "@namics/stylelint-bem": "^10.1.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/node": "^22.15.14",
    "classnames": "^2.5.1",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "express": "^5.1.0",
    "madge": "^8.0.0",
    "normalize-scss": "^8.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6",
    "sass": "^1.86.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "6.0.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "^22.15.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.34",
    "@types/text-encoding": "^0.0.40",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-scss": "^6.11.1",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "text-encoding": "^0.7.0",
    "ts-jest": "29.2.5",
    "typescript": "^5.8.3"
  },
  "license": "MIT"
